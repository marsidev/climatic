{
	"buildCommand": "pnpm build",
	"outputDirectory": "public",
	"installCommand": "pnpm install",
	"cleanUrls": true,
	"framework": null,
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "api/index.ts"
		},
		{
			"source": "/assets/(.*)",
			"destination": "public/assets/$1"
		},
		{
			"source": "/(.*)",
			"destination": "public/index.html"
		}
	]
}
