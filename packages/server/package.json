{
	"author": {
		"name": "Luis Marsiglia",
		"email": "marsiglia.business@gmail.com"
	},
	"name": "@climatic/server",
	"version": "1.0.0",
	"main": "dist/src/server.js",
	"license": "ISC",
	"scripts": {
		"dev": "cross-env NODE_ENV=development tsx watch src/server.ts --host",
		"dev:log": "pnpm dev --log",
		"build": "pnpm clean && tsc",
		"start": "cross-env NODE_ENV=production tsx dist/src/server.js",
		"lint": "eslint . --ext .js --ext .ts",
		"lint:fix": "pnpm lint --fix",
		"clean": "rimraf dist",
		"tsc": "tsc --noEmit",
		"test": "cross-env NODE_ENV=test vitest run",
		"test:watch": "cross-env NODE_ENV=test vitest"
	},
	"dependencies": {
		"@climatic/shared": "workspace:*",
		"@fastify/autoload": "5.0.0",
		"@fastify/cors": "8.0.0",
		"@fastify/sensible": "5.1.0",
		"@fastify/static": "6.4.0",
		"cross-env": "7.0.3",
		"dotenv": "16.0.1",
		"fastify": "4.1.0",
		"fastify-plugin": "^3.0.1",
		"isomorphic-fetch": "3.0.0",
		"node-cache": "5.1.2",
		"pino": "8.1.0",
		"pino-pretty": "8.1.0",
		"tsconfig-paths": "4.0.0"
	},
	"devDependencies": {
		"@types/isomorphic-fetch": "0.0.36",
		"@types/node": "18.0.0",
		"@types/supertest": "2.0.12",
		"eslint": "8.18.0",
		"eslint-plugin-node": "11.1.0",
		"perf_hooks": "0.0.1",
		"picocolors": "1.0.0",
		"rimraf": "3.0.2",
		"supertest": "6.2.3",
		"tsx": "3.5.0",
		"typescript": "4.7.4",
		"vite": "2.9.12",
		"vite-tsconfig-paths": "3.5.0",
		"vitest": "0.15.2"
	},
	"packageManager": "pnpm@7.14.0",
	"type": "commonjs"
}
