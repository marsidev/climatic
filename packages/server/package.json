{
  "author": {
    "name": "Luis Marsiglia",
    "email": "marsiglia.business@gmail.com"
  },
  "name": "@climatic/server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "ISC",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/server.ts",
    "build": "pnpm clean && tsc",
    "start": "cross-env NODE_ENV=production node -r ts-node/register/transpile-only -r tsconfig-paths/register ./dist/server.js",
    "lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx",
    "lint:fix": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx --fix",
    "clean": "rimraf dist",
    "tsc": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "test:watch": "pnpm test -- --watchAll"
  },
  "dependencies": {
    "@climatic/shared": "workspace:^1.0.0",
    "@fastify/autoload": "4.0.1",
    "@fastify/cors": "7.0.0",
    "@fastify/sensible": "4.1.0",
    "@fastify/static": "5.0.2",
    "cross-env": "7.0.3",
    "dotenv": "16.0.1",
    "fastify": "3.29.0",
    "fastify-plugin": "3.0.1",
    "isomorphic-fetch": "3.0.0",
    "node-cache": "5.1.2",
    "pino": "7.11.0",
    "pino-pretty": "7.6.1",
    "ts-node": "10.8.1",
    "tsconfig-paths": "4.0.0"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.36",
    "@types/jest": "28.1.1",
    "@types/node": "17.0.41",
    "@types/supertest": "2.0.12",
    "eslint": "8.17.0",
    "eslint-plugin-node": "11.1.0",
    "jest": "28.1.1",
    "rimraf": "3.0.2",
    "supertest": "6.2.3",
    "ts-jest": "28.0.4",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.3"
  },
  "type": "commonjs"
}
