    "@fastify/sensible": "5.0.0",
    "@fastify/static": "6.4.0",
    "cross-env": "7.0.3",
    "dotenv": "16.0.1",
    "fastify": "4.0.2",
    "fastify-plugin": "^3.0.1",
    "isomorphic-fetch": "3.0.0",
    "node-cache": "5.1.2",
    "pino": "8.0.0",
    "pino-pretty": "8.0.0",
    "tsconfig-paths": "4.0.0"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.36",
    "@types/node": "17.0.43",
    "@types/supertest": "2.0.12",
    "eslint": "8.17.0",
