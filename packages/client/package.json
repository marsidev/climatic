  "name": "@climatic/client",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "dev": "cross-env VITE_ENV_TYPE=dev vite --host",
    "dev:https": "pnpm dev --https",
    "build": "pnpm clean && pnpm tsc && vite build",
    "start": "vite preview",
    "clean": "rimraf ../../dist",
    "lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx",
    "lint:fix": "pnpm lint --fix",
    "tsc": "tsc --noEmit",
    "test": "pnpm test:integration",
    "test:watch": "pnpm test:integration:watch",
    "test:integration": "cross-env VITE_ENV_TYPE=test vitest run",
    "test:integration:watch": "cross-env VITE_ENV_TYPE=test vitest",
    "test:e2e": "npx playwright test",
    "test:e2e:watch": "nodemon --delay 1 -e ts,tsx --exec pnpm test:e2e"
  },
  "dependencies": {
    "@babel/core": "7.18.5",
    "@chakra-ui/react": "2.6.0",
    "@climatic/shared": "workspace:^1.0.0",
    "@emotion/react": "11.10.8",
    "@emotion/styled": "11.10.8",
    "@fontsource/aileron": "4.5.3",
    "@fontsource/nunito": "4.5.12",
    "@fontsource/raleway": "4.5.12",
    "@fontsource/rubik": "4.5.14",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "4.0.0",
    "country-emoji": "1.5.6",
    "country-flag-emoji-polyfill": "0.1.4",
    "framer-motion": "10.12.4",
    "i18next": "22.4.15",
    "isomorphic-fetch": "3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-i18next": "12.2.2",
    "react-icons": "4.4.0",
    "react-router-dom": "6.11.0",
    "react-tooltip": "4.2.21",
    "swr": "1.3.0",
    "use-debounce": "9.0.4",
    "vite-tsconfig-paths": "4.2.0",
    "zustand": "4.3.7"
  },
  "devDependencies": {
    "@chakra-ui/utils": "2.0.14",
    "@playwright/test": "1.31.2",
    "@testing-library/dom": "8.14.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.1",
    "@types/isomorphic-fetch": "0.0.36",
    "@types/node": "18.16.3",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/rimraf": "3.0.2",
    "@types/testing-library__jest-dom": "5.14.5",
    "cross-env": "7.0.3",
    "eslint": "8.39.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "19.0.0",
    "msw": "1.2.1",
    "net": "1.0.2",
    "nodemon": "2.0.22",
    "playwright": "1.31.2",
    "rimraf": "3.0.2",
    "tslib": "2.5.0",
    "typescript": "5.0.4",
    "vite": "4.3.3",
    "vitest": "0.30.1"
  }
}
