  "name": "@climatic/client",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "dev": "cross-env VITE_ENV_TYPE=dev vite --host",
    "dev:https": "cross-env VITE_ENV_TYPE=dev vite --host --https",
    "build": "pnpm clean && pnpm tsc && vite build",
    "start": "vite preview",
    "clean": "rimraf ../../dist",
    "lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx",
    "lint:fix": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx --fix",
    "tsc": "tsc --noEmit",
    "test": "pnpm test:integration",
    "test:watch": "pnpm test:integration:watch",
    "test:integration": "cross-env VITE_ENV_TYPE=test vitest run",
    "test:integration:watch": "cross-env VITE_ENV_TYPE=test vitest",
    "test:e2e": "npx playwright test",
    "test:e2e:watch": "nodemon --delay 1 -e ts,tsx --exec pnpm test:e2e"
  },
  "dependencies": {
    "@babel/core": "7.18.2",
    "@chakra-ui/react": "2.2.1",
    "@climatic/shared": "workspace:^1.0.0",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@fontsource/aileron": "4.5.3",
    "@fontsource/nunito": "4.5.9",
    "@fontsource/raleway": "4.5.9",
    "@fontsource/rubik": "4.5.10",
    "@vitejs/plugin-react": "1.3.2",
    "country-emoji": "1.5.6",
    "country-flag-emoji-polyfill": "0.1.1",
    "framer-motion": "6.3.11",
    "isomorphic-fetch": "3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0",
    "react-router-dom": "6",
    "react-tooltip": "4.2.21",
    "swr": "1.3.0",
    "use-debounce": "8.0.1",
    "vite-tsconfig-paths": "3.5.0",
    "zustand": "4.0.0-rc.1"
  },
  "devDependencies": {
    "@chakra-ui/utils": "2.0.2",
    "@playwright/test": "1.22.2",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.0",
    "@types/isomorphic-fetch": "0.0.36",
    "@types/node": "17.0.43",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/rimraf": "3.0.2",
    "@types/testing-library__jest-dom": "5.14.3",
    "cross-env": "7.0.3",
    "eslint": "8.17.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "jsdom": "19.0.0",
    "msw": "0.42.1",
    "net": "1.0.2",
    "nodemon": "2.0.16",
    "playwright": "1.22.2",
    "rimraf": "3.0.2",
    "tslib": "2.4.0",
    "typescript": "4.7.3",
    "vite": "2.9.12",
    "vitest": "0.15.0"
  }
}
