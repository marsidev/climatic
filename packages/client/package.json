	"name": "@climatic/client",
	"version": "1.0.0",
	"license": "ISC",
	"scripts": {
		"dev": "cross-env VITE_ENV_TYPE=dev vite --host",
		"dev:https": "pnpm dev --https",
		"build": "pnpm clean && pnpm tsc && vite build",
		"start": "vite preview",
		"clean": "rimraf ../../public",
		"lint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx",
		"lint:fix": "pnpm lint --fix",
		"tsc": "tsc --noEmit",
		"test": "pnpm test:integration",
		"test:watch": "pnpm test:integration:watch",
		"test:integration": "cross-env VITE_ENV_TYPE=test vitest run",
		"test:integration:watch": "cross-env VITE_ENV_TYPE=test vitest",
		"test:e2e": "npx playwright test",
		"test:e2e:watch": "nodemon --delay 1 -e ts,tsx --exec pnpm test:e2e"
	},
	"dependencies": {
		"@babel/core": "7.18.5",
		"@chakra-ui/react": "2.2.1",
		"@climatic/shared": "workspace:*",
		"@emotion/react": "11.9.3",
		"@emotion/styled": "11.9.3",
		"@fontsource/aileron": "4.5.3",
		"@fontsource/nunito": "4.5.9",
		"@fontsource/raleway": "4.5.9",
		"@fontsource/rubik": "4.5.10",
		"@vitejs/plugin-react": "2.2.0",
		"country-emoji": "1.5.6",
		"country-flag-emoji-polyfill": "0.1.1",
		"framer-motion": "6.3.11",
		"i18next": "21.8.10",
		"isomorphic-fetch": "3.0.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hook-form": "7.32.2",
		"react-i18next": "11.17.2",
		"react-icons": "4.4.0",
		"react-router-dom": "6",
		"react-tooltip": "4.2.21",
		"swr": "1.3.0",
		"use-debounce": "8.0.1",
		"vite-tsconfig-paths": "3.5.2",
		"zustand": "4.0.0-rc.1"
	},
	"devDependencies": {
		"@chakra-ui/utils": "2.0.2",
		"@playwright/test": "1.22.2",
		"@testing-library/dom": "8.14.0",
		"@testing-library/jest-dom": "5.16.4",
		"@testing-library/react": "13.3.0",
		"@testing-library/user-event": "14.2.1",
		"@types/isomorphic-fetch": "0.0.36",
		"@types/node": "18.0.0",
		"@types/react": "18.0.14",
		"@types/react-dom": "18.0.5",
		"@types/rimraf": "3.0.2",
		"@types/testing-library__jest-dom": "5.14.5",
		"cross-env": "7.0.3",
		"eslint": "8.18.0",
		"eslint-plugin-react": "7.30.0",
		"eslint-plugin-react-hooks": "4.6.0",
		"jsdom": "19.0.0",
		"msw": "0.42.1",
		"net": "1.0.2",
		"nodemon": "2.0.16",
		"rimraf": "3.0.2",
		"tslib": "2.4.0",
		"typescript": "4.8.4",
		"vite": "3.2.2",
		"vitest": "0.24.5"
	},
	"packageManager": "pnpm@7.14.0"
}
